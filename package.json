    "vue-router": "^4.0.0-beta.4"
  },
  "devDependencies": {
    "@vue/compiler-sfc": "^3.0.0-rc.5",
    "json-server": "^0.16.1",
    "vite": "^1.0.0-rc.4"
  }
}
